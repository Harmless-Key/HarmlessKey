{
  "name": "harmlesskey_app",
  "version": "2.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "postbuild": "react-snap",
    "docker-build": "docker build -t harmlesskey/harmlesskey .",
    "docker-push": "docker push harmlesskey/harmlesskey:latest",
    "docker-run": "docker run -p 8081:80 -d --name Harmlesskey harmlesskey/harmlesskey"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.13",
    "@fortawesome/free-solid-svg-icons": "^5.7.0",
    "@fortawesome/vue-fontawesome": "^0.1.5",
    "@quasar/extras": "^1.0.0",
    "ajv": "^8.10.0",
    "ajv-formats": "^2.1.1",
    "animate.css": "^4.1.1",
    "axios": "^0.21.4",
    "core-js": "^2.6.5",
    "firebase": "^8.0.0",
    "lodash": "^4.17.21",
    "node": "^11.0.0",
    "quasar": "^1.0.0",
    "react-snap": "^1.23.0",
    "register-service-worker": "^1.7.1",
    "vee-validate": "^3.4.14",
    "vue": "^2.5.17",
    "vue-analytics": "^5.22.1",
    "vue-cookies": "^1.5.7",
    "vue-google-adsense": "^1.10.1",
    "vue-markdown": "^2.2.4",
    "vue-meta": "^2.4.0",
    "vue-numeral-filter": "^1.1.1",
    "vue-qr": "^1.5.3",
    "vue-router": "^3.0.1",
    "vue-script2": "^2.1.0",
    "vue-shortkey": "^3.1.6",
    "vue-snotify": "^3.2.1",
    "vue2-flip-countdown": "^0.12.1",
    "vuebar": "0.0.20",
    "vuefire": "^1.4.5",
    "vuex": "^3.0.1",
    "workbox-sw": "^6.0.2"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.7.0",
    "@fortawesome/fontawesome-pro": "^5.13.1",
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-plugin-pwa": "^4.5.10",
    "@vue/cli-service": "^4.5.9",
    "babel-plugin-transform-imports": "1.5.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^8.0.0",
    "style-loader": "^0.23.1",
    "vue-cli-plugin-quasar": "~2.0.2",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "engines": {
    "node": "8.10.0",
    "npm": "6.4.1"
  },
  "reactSnap": {
    "source": "dist",
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "include": [
      "/",
      "/about-us",
      "/privacy-policy",
      "/patreon",
      "/compendium",
      "/compendium/conditions",
      "/compendium/monsters",
      "/compendium/spells",
      "/compendium/items",
      "/compendium/conditions/blinded",
      "/compendium/monsters/aboleth",
      "/compendium/spells/acid-arrow"
    ]
  }
}
